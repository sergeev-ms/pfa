<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://analyticSetEdit.caption"
        focusComponent="form"
        messagesPack="com.borets.pfa.web.screens.analytic.analyticset">
    <data>
        <instance id="analyticSetDc" class="com.borets.pfa.entity.analytic.AnalyticSet">
            <view extends="_local"/>
            <loader/>
        </instance>
    </data>
    <dialogMode height="600" width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="analyticSetDc">
            <column width="450px">
                <lookupField id="jobTypeField" property="jobType"/>
                <lookupField id="wellTagField" property="wellTag"/>
                <lookupField id="wellEquipField" property="wellEquip"/>
                <lookupField id="contractTypeField" property="contractType"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button id="commitAndCloseBtn" action="windowCommitAndClose"/>
            <button id="closeBtn" action="windowClose"/>
        </hbox>
    </layout>
</window>
