<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://intakeConfigBrowse.caption" focusComponent="intakeConfigsTable"
        messagesPack="com.borets.pfa.web.screens.account.system.classification.intakeconfig">
    <data readOnly="true">
        <collection id="intakeConfigsDc" class="com.borets.pfa.entity.account.system.classification.IntakeConfig">
            <view extends="_local"/>
            <loader id="intakeConfigsDl">
                <query>
                    <![CDATA[select e from pfa_IntakeConfig e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600" width="800"/>
    <layout expand="intakeConfigsTable" spacing="true">
        <filter id="filter" applyTo="intakeConfigsTable" dataLoader="intakeConfigsDl">
            <properties include=".*"/>
        </filter>
        <groupTable id="intakeConfigsTable" width="100%" dataContainer="intakeConfigsDc">
            <actions>
                <action id="create" type="create"/>
                <action id="edit" type="edit"/>
                <action id="remove" type="remove"/>
            </actions>
            <columns>
                <column id="name"/>
            </columns>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel" alwaysVisible="true">
                <button id="createBtn" action="intakeConfigsTable.create"/>
                <button id="editBtn" action="intakeConfigsTable.edit"/>
                <button id="removeBtn" action="intakeConfigsTable.remove"/>
            </buttonsPanel>
        </groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
