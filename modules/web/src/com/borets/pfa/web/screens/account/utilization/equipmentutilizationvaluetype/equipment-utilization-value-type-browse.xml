<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://equipmentUtilizationValueTypeBrowse.caption"
        focusComponent="equipmentUtilizationValueTypesTable"
        messagesPack="com.borets.pfa.web.screens.account.utilization.equipmentutilizationvaluetype">
    <data readOnly="true">
        <collection id="equipmentUtilizationValueTypesDc"
                    class="com.borets.pfa.entity.account.utilization.EquipmentUtilizationValueType">
            <view extends="_local"/>
            <loader id="equipmentUtilizationValueTypesDl">
                <query>
                    <![CDATA[select e from pfa_EquipmentUtilizationValueType e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600" width="800"/>
    <layout expand="equipmentUtilizationValueTypesTable" spacing="true">
        <filter id="filter" applyTo="equipmentUtilizationValueTypesTable" dataLoader="equipmentUtilizationValueTypesDl">
            <properties include=".*"/>
        </filter>
        <groupTable id="equipmentUtilizationValueTypesTable" width="100%"
                    dataContainer="equipmentUtilizationValueTypesDc" multiselect="true">
            <actions>
                <action id="create" type="create"/>
                <action id="edit" type="edit"/>
                <action id="remove" type="remove"/>
            </actions>
            <columns>
                <column id="name"/>
            </columns>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel" alwaysVisible="true">
                <button id="createBtn" action="equipmentUtilizationValueTypesTable.create"/>
                <button id="editBtn" action="equipmentUtilizationValueTypesTable.edit"/>
                <button id="removeBtn" action="equipmentUtilizationValueTypesTable.remove"/>
            </buttonsPanel>
        </groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
