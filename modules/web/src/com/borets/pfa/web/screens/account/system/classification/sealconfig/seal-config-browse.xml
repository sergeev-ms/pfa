<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://sealConfigBrowse.caption" focusComponent="sealConfigsTable"
        messagesPack="com.borets.pfa.web.screens.account.system.classification.sealconfig">
    <data readOnly="true">
        <collection id="sealConfigsDc" class="com.borets.pfa.entity.account.system.classification.SealConfig">
            <view extends="_local"/>
            <loader id="sealConfigsDl">
                <query>
                    <![CDATA[select e from pfa_SealConfig e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600" width="800"/>
    <layout expand="sealConfigsTable" spacing="true">
        <filter id="filter" applyTo="sealConfigsTable" dataLoader="sealConfigsDl">
            <properties include=".*"/>
        </filter>
        <groupTable id="sealConfigsTable" width="100%" dataContainer="sealConfigsDc">
            <actions>
                <action id="create" type="create"/>
                <action id="edit" type="edit"/>
                <action id="remove" type="remove"/>
            </actions>
            <columns>
                <column id="name"/>
            </columns>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel" alwaysVisible="true">
                <button id="createBtn" action="sealConfigsTable.create"/>
                <button id="editBtn" action="sealConfigsTable.edit"/>
                <button id="removeBtn" action="sealConfigsTable.remove"/>
            </buttonsPanel>
        </groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
